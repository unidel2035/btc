  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "collect": "tsx src/collectors/index.ts",
    "analyze": "tsx src/analyzers/index.ts",
    "backtest": "tsx src/trading/backtest.ts",
    "dashboard": "tsx src/dashboard/server.ts",
    "test": "npm run test:all",
    "test:all": "npm run test:sentiment && npm run test:risk && npm run test:strategies && npm run test:paper && npm run test:dashboard && npm run test:backtest && npm run test:notifications && npm run test:database && npm run test:social:retry && npm run test:social:orchestrator && npm run test:exchanges && npm run test:screening && npm run test:screening-integram && npm run test:telegram",
    "test:jest": "jest",
    "test:coverage": "jest --coverage",
    "test:sentiment": "tsx tests/analyzers/sentiment.test.ts",
    "test:risk": "tsx tests/trading/risk.test.ts",
    "test:strategies": "tsx tests/trading/strategies.test.ts",
    "test:dashboard": "tsx tests/dashboard/dashboard.test.ts",
    "test:backtest": "tsx tests/trading/backtest.test.ts",
    "test:notifications": "tsx tests/notifications/notifications.test.ts",
    "test:database": "tsx tests/database/database.test.ts",
    "test:paper": "tsx tests/trading/paper.test.ts",
    "test:social:retry": "tsx tests/collectors/social/retry.test.ts",
    "test:social:orchestrator": "tsx tests/collectors/social/orchestrator.test.ts",
    "test:exchanges": "tsx tests/exchanges/exchanges.test.ts",
    "test:screening": "tsx tests/analyzers/screening.test.ts",
    "test:screening-integram": "tsx tests/database/screening.test.ts",
    "test:telegram": "tsx tests/telegram/telegram-bot.test.ts",
    "example:news": "tsx examples/news-collector-example.ts",
    "example:sentiment": "tsx examples/sentiment-analyzer-example.ts",
    "example:risk": "tsx examples/risk-manager-example.ts",
    "example:strategies": "tsx examples/strategies-example.ts",
    "example:dashboard": "tsx examples/dashboard-example.ts",
    "example:backtest": "tsx examples/backtest-example.ts",
    "example:notifications": "tsx examples/notifications-example.ts",
    "example:database": "tsx examples/database-example.ts",
    "example:paper": "tsx examples/paper-trading-example.ts",
    "example:twitter": "tsx examples/twitter-collector-example.ts",
    "example:reddit": "tsx examples/reddit-collector-example.ts",
    "example:social": "tsx examples/social-orchestrator-example.ts",
    "example:exchanges": "tsx examples/exchanges-example.ts",
    "example:integram": "tsx examples/integram-example.ts",
    "example:screening": "tsx examples/screening-example.ts",
    "example:screening-integram": "tsx examples/screening-integram-example.ts",
    "example:telegram": "tsx examples/telegram-bot-example.ts",
    "screening": "tsx examples/screening-example.ts",
    "db:migrate": "tsx src/database/migrate.ts",
    "db:seed": "tsx src/database/seed.ts",
    "db:backup": "tsx src/database/backup.ts",
    "integram:setup": "tsx scripts/setup-integram.ts"
  },
  "keywords": [
    "bitcoin",
    "trading",
    "bot",
    "cryptocurrency",
    "news",
    "sentiment-analysis",
    "algorithmic-trading",
    "social-media-monitoring"
  ],
  "author": "",
  "license": "MIT",
