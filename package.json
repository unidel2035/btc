{
  "name": "btc-trading-bot",
  "version": "0.1.0",
  "description": "Торговый робот криптовалюты на основе анализа новостных лент, социальных сигналов и технической аналитики",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "collect": "tsx src/collectors/index.ts",
    "analyze": "tsx src/analyzers/index.ts",
    "backtest": "tsx src/trading/backtest.ts",
    "dashboard": "tsx src/dashboard/server.ts",
    "test": "echo \"Tests not yet implemented\" && exit 0"
  },
  "keywords": [
    "bitcoin",
    "trading",
    "bot",
    "cryptocurrency",
    "news",
    "sentiment-analysis",
    "algorithmic-trading"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.4",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "snoowrap": "^1.23.0",
    "telegraf": "^4.16.3",
    "twitter-api-v2": "^1.28.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
